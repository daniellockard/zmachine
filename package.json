{
  "name": "zmachine",
  "version": "0.1.0",
  "description": "A Z-machine emulator written from scratch in TypeScript",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:web": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "dev": "vite",
    "dev:web": "vite --config vite.config.ts"
  },
  "keywords": [
    "zmachine",
    "z-machine",
    "infocom",
    "interactive-fiction",
    "emulator",
    "interpreter"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "vite": "^5.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
  }
}
